{
  "private": true,
  "name": "nlgo_dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "plugins": ["@babel"],
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.0",
    "eslint": "^8.30.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^27.0.0",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.4.2",
    "sirv-cli": "^2.0.2"
  },
  "dependencies": {
    "chart.js": "^4.2.1",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^3.2.1",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.10.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
